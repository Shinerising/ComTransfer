<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
    </startup>
	<appSettings>
		<add key="com" value="1"/>
		<add key="baudrate" value="115200"/>
		<add key="databits" value="8"/>
		<add key="stopbits" value="1"/>
		<add key="parity" value="NONE"/>
		<add key="ishw" value="false"/>
		<add key="issw" value="false"/>
		<add key="isdtr" value="true"/>
		<add key="isrts" value="true"/>
		<add key="directory" value="*&gt;C:\temp"/>
	</appSettings>
	<runtime>
		<AppContextSwitchOverrides value="Switch.System.Windows.DoNotScaleForDpiChanges=false"/>
	</runtime>
</configuration>
